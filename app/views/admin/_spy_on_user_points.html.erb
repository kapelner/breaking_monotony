<canvas id="canvas" width="<%= @identification.image.width %>" height="<%= @identification.image.height %>"></canvas>
<img src="<%= url_for :controller => :files, :action => :get_training_image, :id => @identification.image.encrypted_id %>"
  border="0"
  width="<%= @identification.image.width %>"
  height="<%= @identification.image.height %>"
  id="original_training_image"
  style="display:none;" />
<script type="text/javascript">
  var canvas = $("canvas");
  var training_image = $('original_training_image');
  var ctx = canvas.getContext("2d");

  Event.observe(window, 'load', function(){
    redraw_image();
  });

  var user_pt_color = "rgb(255, 255, 0)";
  var white = "rgb(255, 255, 255)";
  var black = "rgb(0, 0, 0)";
  var red = "rgb(255, 0, 0)";

  function redraw_image(){
    //draw image:
    ctx.drawImage(training_image, 0, 0, training_image.width, training_image.height);
    //draw user points:
    <% points.each do |p| %>
      draw_star(<%= p %>, user_pt_color);
    <% end %>
    <% @identification.image.truth_points.each_with_index do |p, i| %>
      <% if params[:r] %>
        draw_ring(<%= p %>, <%= params[:r] %>);
      <% else %>
        draw_cross(<%= p %>, 2, black);
      <% end %>
      //alert("#<%= i + 1 %>: <%= p %>");
    <% end %>
  }

  function draw_cross(x, y, max_r, color){
    //round the points
    x = Math.round(x);
    y = Math.round(y);
    ctx.fillStyle = color;
    ctx.fillRect(x, y, 1, 1);
    for (var r = 0; r < max_r; r++){
      ctx.fillRect(x, y - r, 1, 1);
      ctx.fillRect(x, y + r, 1, 1);
      ctx.fillRect(x + r, y, 1, 1);
      ctx.fillRect(x - r, y, 1, 1);
      ctx.fillRect(x + r, y + r, 1, 1);
      ctx.fillRect(x - r, y + r, 1, 1);
      ctx.fillRect(x + r, y - r, 1, 1);
      ctx.fillRect(x - r, y - r, 1, 1);
    }
  }

  function draw_star(x, y, color){
    //round the points
    x = Math.round(x);
    y = Math.round(y);
    //draw the specific color as an outer shell star shape
    ctx.fillStyle = color;
    ctx.fillRect (x, y - 3, 1, 1);
    ctx.fillRect (x, y + 3, 1, 1);
    ctx.fillRect (x + 3, y, 1, 1);
    ctx.fillRect (x - 3, y, 1, 1);
    ctx.fillRect (x, y - 4, 1, 1);
    ctx.fillRect (x, y + 4, 1, 1);
    ctx.fillRect (x + 4, y, 1, 1);
    ctx.fillRect (x - 4, y, 1, 1);
    ctx.fillRect (x - 1, y - 2, 1, 1);
    ctx.fillRect (x - 1, y + 2, 1, 1);
    ctx.fillRect (x + 1, y - 2, 1, 1);
    ctx.fillRect (x + 1, y + 2, 1, 1);
    ctx.fillRect (x - 2, y - 1, 1, 1);
    ctx.fillRect (x - 2, y + 1, 1, 1);
    ctx.fillRect (x + 2, y - 1, 1, 1);
    ctx.fillRect (x + 2, y + 1, 1, 1);
    //now draw some white in the middle
    ctx.fillStyle = this.white;
    ctx.fillRect (x, y - 2, 1, 1);
    ctx.fillRect (x, y + 2, 1, 1);
    ctx.fillRect (x + 2, y, 1, 1);
    ctx.fillRect (x - 2, y, 1, 1);
    ctx.fillRect (x - 1, y - 1, 1, 1);
    ctx.fillRect (x - 1, y + 1, 1, 1);
    ctx.fillRect (x + 1, y - 1, 1, 1);
    ctx.fillRect (x + 1, y + 1, 1, 1);
    //and a black cross in the center
    ctx.fillStyle = this.black;
    ctx.fillRect (x, y - 1, 1, 1);
    ctx.fillRect (x, y + 1, 1, 1);
    ctx.fillRect (x + 1, y, 1, 1);
    ctx.fillRect (x - 1, y, 1, 1);
    ctx.fillRect (x, y, 1, 1);
  }

